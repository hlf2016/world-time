{
  "include": [
    "auto-imports.d.ts",
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    // 支持jsx语法 才能直接使用 components
    "jsx": "preserve",
    "paths": {
      "~/*": [
        "./src/*"
      ]
    },
    "types": [
      "vite/client"
    ]
  },
  "exclude": [
    "node_modules",
    "**/dist/**"
  ]
}
